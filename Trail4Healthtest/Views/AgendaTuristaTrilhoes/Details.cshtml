@model Trail4Healthtest.Models.AgendaTuristaTrilho

@{
    ViewData["Title"] = "Details";
}

<div class="bread_area">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <ol class="breadcrumb">
                    <li><a href="/">Trail4Health</a></li>
                    <li class="active">Detalhes da Agenda</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<div class="container">
    <div class="row">
        <section class="page col-sm-12">
            <div class="row">
                <div class="col-lg-12">
                    <hr />
                    <dl class="dl-horizontal">
                        <dt>
                            Data Ínicio
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Datainicio)
                        </dd>
                        <dt>
                            Data Fim
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Datafim)
                        </dd>
                        <dt>
                            Tempo Gasto
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Tempogasto)
                        </dd>
                        <dt>
                            Nome Trilho
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Trilho.Nometrilho)
                        </dd>
                        <dt>
                            Turista
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.Turista.Nome)
                        </dd>
                    </dl>
                </div>
                <div class="form-group">
                    <button class="btn btn-toolbar"><a asp-action="Edit" asp-route-id="@Model.AgendaId">Editar</a></button> | <button class="btn btn-info"><a asp-action="Index">Voltar à lista</a></button>
                </div>
            </div>
        </section>
    </div>
</div>

